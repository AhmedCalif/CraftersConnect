    <script>

        

        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for(var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }

        
        function interactSite(content_type, content_id, user_interaction) {
            $.post("https://api.alphacoders.com/site/interact", {

                content_type: content_type,
                content_id: content_id,
                user_interaction: user_interaction
            },
            function(data){
                if(data.hasOwnProperty('status') && data.status == "success") {
                   
                } else if(data.hasOwnProperty('error_message')) {
                  
                }
            }, "json");
        }

        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires="+d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";domain=.alphacoders.com;path=/;secure";
        }


        $(document).ready(function() {
            var cookie_status = getCookie("cookieconsent_status");
            if(cookie_status == "" || cookie_status == "deny") {
                $('#cookie_consent').css('display', 'flex');
            }

            $('#change_consent').on('click', function () {
                $('#cookie_consent').css('display', 'flex');
            });

            $('#cookie_consent .btn-allow').on('click', function () {
                setCookie('cookieconsent_status', 'allow', 365);
                $('#cookie_consent').css('display', 'none');
            });

            $('#cookie_consent .btn-deny').on('click', function () {
                setCookie('cookieconsent_status', 'deny', 365);
                window.location.href = "https://alphacoders.com/site/no-consent";
            });

            if($("#sorting_options").length) {
                $("#sorting_options").on('shown.bs.collapse', function() {
                    fixVideoAd('shown', $('#sorting_options').outerHeight() + 15);
                }).on('hide.bs.collapse', function() {
                    fixVideoAd('hidden', $('#sorting_options').outerHeight() + 15);
                });
            }

            $('body').on('click', '.close-notice', function() {
                $(this).parent().fadeOut(600, function(){$(this).remove();});
            });
        });

        function fixVideoAd(action, height) {
            //Ad video has been detect
            if($('div[id^="clone_teaser_"]').length) {

                var ad_div = $('div[id^="teaser_"]');

                if(action == "shown") {
                    ad_div.css("top", parseInt(ad_div.css('top').replace(/[^-\d\.]/g, '')) + height);
                }
                else {
                    ad_div.css("top", parseInt(ad_div.css('top').replace(/[^-\d\.]/g, '')) - height);
                }
            }
        }

        function showWebsites() {
            if(!$("#show_websites").is(':empty')) {
                fixVideoAd('hidden', $('#show_websites').outerHeight());

                $("#show_websites").empty();
            }
            else {
                $.get("/get_header_websites.php",
                    function(data) {
                        $("#show_websites").html(data);

                        fixVideoAd('shown', $('#show_websites').outerHeight());
                    }
                );
            }
        }

        function handleApiError(error_message) {
            if(!$('.notice-error').length) {
                $('#page_container').prepend("<div class='notice notice-error'>" +
                    "<span class='close-notice pull-right' title=\"Close This Notification!\">" +
                        "<i class=\"el el-remove\"></i>" +
                    "</span>" +
                    "<b></b>" +
                    "</div>")
            }

            $('.notice-error b').html(error_message);
            $('html,body').animate({scrollTop: $('.notice-error').offset().top - 15});
        }
    </script>

    <script>
        function profileFilter(profile_user_id, profile_user_name, filter_type) {
            const form = document.createElement("form");
            form.method = "POST";
            form.action = "https://alphacoders.com/users/profile/" + profile_user_id + "/" + encodeURIComponent(profile_user_name);
            
            const hidden_field = document.createElement("input");
            hidden_field.type = "hidden";
            hidden_field.name = "filter";
            hidden_field.value = filter_type;
            
            form.appendChild(hidden_field);
            document.body.appendChild(form);
            
            form.submit();
        }


    </script>

        

    <script>
        let is_loading = 0;
        let loading_more_next = 0;
                const max_displayed = 8;
                let current_page = 1;
        let next_page = 2;
        // Last page is either set, or defaults to 0, in which case we don't know what the last page is, as we didn't get a count on the page.
                    let last_page = 0;
                
        let clean_timer;

        const title_next_disabled = "Auto loading the next pages is disabled!";
        const title_next_enabled = "Auto loading the next pages is enabled!";

        //Load the next page of results
        function loadingNext() {
            $('#loader_next').show();

            let next_url_base = "https://wall.alphacoders.com/related_wallpapers.php?sub_category_id=174912";
            let next_url_params = "";

            // ? or & based on if there's already content in the query bit of the url
                            next_url_params += "&";
            
                            next_url_params += "quickload=1&page=" + next_page; 
            
            // We're splitting up the single url into next_url_base and next_url_params (all the info for quick load) so that Google doesn't crawl the 
            // quickload pages. I'm not sure if it would still be an issue with params setup the way they are now, but it doesn't hurt.
            $.get(next_url_base + next_url_params,
                function(data) {

                    // Lets try and see if our next page returned any wallpapers
                    let next_page_returned_wallpapers = false;

                    if(data.search("thumb-container") !== -1) {
                        next_page_returned_wallpapers = true;
                    } 

                    if(next_page_returned_wallpapers) {
